{
  "name": "vue-cli-project-demo",
  "version": "0.1.0",
  "keywords": [
    "vue",
    "demo",
    "packages"
  ],
  "main": "lib/vue-cli-project-demo.umd.js",
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "build": "yarn build:lib && yarn build:components",
    "build:lib": "vue-cli-service build --target lib --dest lib ./src/entry.ts && yarn removeHtml",
    "build:components": "webpack --config webpack.component.js",
    "removeHtml": "rm ./lib/*.html",
    "lint:js": "vue-cli-service lint",
    "lint:css": "npx stylelint src packages --fix",
    "test:unit": "vue-cli-service test:unit"
  },
  "pre-commit": [
    "lint:js",
    "lint:css"
  ],
  "files": [
    "dist",
    "lib",
    "docs",
    "types"
  ],
  "typings": "types/index.d.ts",
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-plugin-typescript": "^3.9.0",
    "@vue/cli-plugin-unit-mocha": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "pre-commit": "^1.2.2",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.6"
  },
  "author": {
    "name": "CaiYin",
    "email": "c763416559@gmail.com"
  },
  "homepage": "https://github.com/kazehaiya/vue-cli-project-demo",
  "repository": {
    "type": "git",
    "url": "https://github.com/kazehaiya/vue-cli-project-demo"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "private": false,
  "license": "MIT"
}
